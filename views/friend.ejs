<%- include header %>
<ul class="archive">
<div class="my_container_friend">

<% users.forEach(function (other_user, index) { %>


	<% 
		var flag = 0;
		for( friend in user.friendlists) {
			if (user.friendlists[friend].friendlist == other_user.name) { 
				flag = 1;
			}	
		} 
	%>
	<% if (flag != 1) { %>
	 <div class="my_work_friend">
	  <a href="/u/<%= other_user.name %>"><%= other_user.name %></a>
		<form method="post">
		 <input type="hidden" name="username" value="<%= other_user.name %>">
		 <button type="submit" class="btn btn-default" value="加朋友">加朋友</button>
		</form>
	</div>
	<% } %>

 
<% }) %>
</div>


</ul>
<%- include footer %>







